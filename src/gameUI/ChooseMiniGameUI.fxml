<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameUI.ChooseMiniGameController">
   <center>
      <VBox prefHeight="400.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="79.0" prefWidth="700.0" />
            <HBox alignment="CENTER" prefHeight="233.0" prefWidth="700.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="233.0" prefWidth="733.0">
                     <children>
                        <Button fx:id="calculadolaGameButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Calculadola">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="questionIsGameButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Question is">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="makeIt24GameButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Make it 24" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
